<% if f.object.multiple? key %>
  <%= f.input :description, as: :multi_value, label: 'Description', input_html: { rows: '14', type: 'textarea'}, required: f.object.required?(key) %>
<% else %>
  <%= f.input :description, as: :text, label: 'Description',input_html: { rows: '14' }, required: f.object.required?(key) %>
<% end %>
